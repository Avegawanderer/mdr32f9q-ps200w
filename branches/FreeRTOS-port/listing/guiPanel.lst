N//-------------------------------------------------------//
N// Panel key handler
N//
N// Returns GUI_EVENT_ACCEPTED if key is processed,
N//         GUI_EVENT_DECLINE otherwise
N//-------------------------------------------------------//
Nuint8_t guiPanel_ProcessKey(guiPanel_t *panel, uint8_t key)
N{
N    guiGenericWidget_t *w;
N    if (panel->isFocused)
N    {
N        if (key == PANEL_KEY_SELECT)
X        if (key == 0x01)
N            guiCore_RequestFocusNextWidget((guiGenericContainer_t *)panel,1);
N        else
N            return GUI_EVENT_DECLINE;
X            return 0x00;
N    }
N    else
N    {
N        // Key event came from child elements
N        w = panel->widgets.elements[ panel->widgets.focusedIndex ];
N        if (key == PANEL_KEY_PREV)
X        if (key == 0x03)
N        {
N            // Check if event should be passed to parent
N            if ((panel->focusFallsThrough) && (guiCore_CheckWidgetTabIndex(w) == TABINDEX_IS_MIN))
X            if ((panel->focusFallsThrough) && (guiCore_CheckWidgetTabIndex(w) == 1))
N                return GUI_EVENT_DECLINE;
X                return 0x00;
N            guiCore_RequestFocusNextWidget((guiGenericContainer_t *)panel,-1);
N        }
N        else if (key == PANEL_KEY_NEXT)
X        else if (key == 0x04)
N        {
N            // Check if event should be passed to parent
N            if ((panel->focusFallsThrough) && (guiCore_CheckWidgetTabIndex(w) == TABINDEX_IS_MAX))
X            if ((panel->focusFallsThrough) && (guiCore_CheckWidgetTabIndex(w) == 2))
N                return GUI_EVENT_DECLINE;
X                return 0x00;
N            guiCore_RequestFocusNextWidget((guiGenericContainer_t *)panel,1);
N        }
N        else if (key == PANEL_KEY_ESC)
X        else if (key == 0x02)
N        {
N            if (panel->focusFallsThrough)
N                return GUI_EVENT_DECLINE;
X                return 0x00;
N            //guiPanel_SetFocused(panel, 1);
N            guiCore_SetFocused((guiGenericWidget_t *)panel,1);
N        }
N        else
N        {
N            return GUI_EVENT_DECLINE;   // unknown key
X            return 0x00;   
N        }
N    }
N    return GUI_EVENT_ACCEPTED;
X    return 0x01;
N}
